<view class="container"><tui-grid vue-id="1" onVueInit="__l"><block a:for="{{iconList}}" a:for-item="item" a:for-index="index" a:key="index"><block><tui-grid-item vue-id="{{('2-'+index)+','+('1')}}" onVueInit="__l"><view class="tui-grid-icon"><tui-icon vue-id="{{('3-'+index)+','+('2-'+index)}}" name="{{item.name}}" size="{{item.size}}" color="{{item.color}}" onVueInit="__l"></tui-icon></view><text class="tui-grid-label">{{item.name}}</text></tui-grid-item></block></block></tui-grid></view>