<view class="page"><scroll-view class="scrollList" style="{{'height:'+(winHeight+'px')+';'}}" scroll-y scroll-into-view="{{scrollViewId}}"><view class="search-bar"><view class="search-bar-form"><view class="search-bar-box"><icon class="icon-search-in-box" type="search" size="{{16}}"></icon><input class="search-bar-input" confirm-type="search" placeholder="请输入搜索关键词" placeholder-class="phcolor" data-event-opts="{{[['input',[['inputTyping',['$event']]]],['confirm',[['searchCity',['$event']]]]]}}" value="{{inputVal}}" onInput="__e" onConfirm="__e"/><block a:if="{{inputShowed}}"><view data-event-opts="{{[['tap',[['clearInput',['$event']]]]]}}" class="icon-clear" onTap="__e"><icon type="clear" size="{{15}}"></icon></view></block></view></view><button class="btn-primary" hover-class="btn-hover" data-event-opts="{{[['tap',[['searchCity',['$event']]]]]}}" onTap="__e">搜索</button></view><block a:if="{{inputShowed}}"><view class="tui-list search-result"><block a:for="{{searchResult}}" a:for-item="item" a:for-index="index" a:key="index"><view class="tui-list-cell" hover-class="tui-list-cell-hover" data-name="{{item}}" hover-stay-time="{{150}}" data-event-opts="{{[['tap',[['selectCity',['$event']]]]]}}" onTap="__e"><view class="tui-list-cell-navigate"><image class="img" src="{{'../../static/images/news/'+(index%2===0&&index!==0?'avatar_2.jpg':'avatar_1.jpg')}}"></image><view class="content"><view class="title">{{item.name}}</view><view class="sub-title">{{item.mobile}}</view></view></view></view></block></view></block><block a:if="{{!inputShowed}}"><view><view class="tui-list city-list"><block a:for="{{lists}}" a:for-item="list" a:for-index="index" a:key="index"><block a:if="{{list.data[0]}}"><block><view class="tui-list-cell-divider" id="{{list.letter}}">{{''+list.letter+''}}</view><block a:for="{{list.data}}" a:for-item="item" a:for-index="index2" a:key="index2"><view class="tui-list-cell" hover-class="tui-list-cell-hover" data-name="{{item.name}}" hover-stay-time="{{150}}" data-event-opts="{{[['tap',[['selectCity',['$event']]]]]}}" onTap="__e"><view class="{{(('tui-list-cell-navigate')+' '+(list.data.length-1==index2?'last':''))}}"><image class="img" src="{{'../../static/images/news/'+(index2%2===0&&index2!==0?'avatar_1.jpg':'avatar_2.jpg')}}"></image><view class="content"><view class="title">{{item.name}}</view><view class="sub-title">{{item.mobile}}</view></view></view></view></block></block></block></block></view></view></block></scroll-view><block a:if="{{!inputShowed}}"><view data-event-opts="{{[['touchStart',[['touchStart',['$event']]]],['touchMove',[['touchMove',['$event']]]],['touchEnd',[['touchEnd',['$event']]]],['touchCancel',[['touchCancel',['$event']]]]]}}" class="tui-indexed-list-bar" style="{{'height:'+(indexBarHeight+'px')+';'}}" onTouchStart="__e" catchTouchMove="__e" catchTouchEnd="__e" catchTouchCancel="__e"><block a:for="{{lists}}" a:for-item="items" a:for-index="index" a:key="index"><text class="tui-indexed-list-text" style="{{'height:'+(indexBarItemHeight+'px')+';'}}">{{''+(items.letter=="well"?"#":items.letter)+''}}</text></block></view></block><block a:if="{{touchmove&&lists[touchmoveIndex].letter}}"><view class="tui-indexed-list-alert">{{''+(lists[touchmoveIndex].letter=="well"?"#":lists[touchmoveIndex].letter)+''}}</view></block></view>