<view class="maps-container"><view class="search-bar"><view class="search-bar-form"><view class="search-bar-box"><icon class="icon-search-in-box" type="search" size="16"></icon><input class="search-bar-input" confirm-type="search" placeholder="请输入您的目的地" focus="{{inputShowed}}" data-event-opts="{{[['confirm',[['bindInput',['$event']]]],['input',[['inputTyping',['$event']]]]]}}" value="{{inputVal}}" onConfirm="__e" onInput="__e"/><block a:if="{{inputVal}}"><view data-event-opts="{{[['tap',[['clearInput',['$event']]]]]}}" class="icon-clear" onTap="__e"><icon type="clear" size="14"></icon></view></block></view><label data-event-opts="{{[['tap',[['showInput',['$event']]]]]}}" hidden="{{!(!inputShowed)}}" class="search-bar-label" onTap="__e"><icon class="icon-search" type="search" size="16"></icon><view class="search-bar-text">请输入您的目的地</view></label></view><view data-event-opts="{{[['tap',[['hideInput',['$event']]]]]}}" hidden="{{!(inputShowed)}}" class="cancel-btn" onTap="__e">取消</view></view><map latitude="{{lat}}" longitude="{{lng}}" markers="{{covers}}" scale="{{12}}" onMarkerTap="marker"></map><scroll-view class="scrollView" style="{{'height:'+(scrollH+'px')+';'}}" scroll-y><view class="tui-list"><block a:for="{{address}}" a:for-item="item" a:for-index="index" a:key="index"><view class="{{(('tui-list-cell')+' '+(index==address.length-1?'tui-cell-last':''))}}"><view class=" addr-title ">{{item.title}}</view><view class="addr-box "><view class="addr-detail "><text class="distance">{{item.distance+"m"}}</text>{{''+item.address+''}}</view><view class="addr-opera "><block a:if="{{item.tel}}"><view class="opera-box " hover-class="opcity " hover-stay-time="{{150}}" data-id="{{item.id}}" data-event-opts="{{[['tap',[['call',['$event']]]]]}}" onTap="__e"><image class="mini-img" src="../../static/images/my/call.png "></image><view class="text">打电话</view></view></block><view class="opera-box " hover-class="opcity" hover-stay-time="{{150}}" data-id="{{item.id}}" data-event-opts="{{[['tap',[['go',['$event']]]]]}}" onTap="__e"><image class="mini-img" src="../../static/images/my/go.png"></image><view class="text">到这里</view></view></view></view></view></block></view></scroll-view></view>