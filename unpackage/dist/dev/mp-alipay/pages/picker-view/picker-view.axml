<view class="conatiner"><view class="search-box"><view class="s-input"><image class="s-img" src="../../static/images/index/search.png"></image><input class="input" placeholder-class="phcolor" name="input" placeholder="请输入关键字" confirm-type="search" data-event-opts="{{[['confirm',[['search',['$event']]]],['input',[['bindViewInput',['$event']]]]]}}" value="{{searchKey}}" onConfirm="__e" onInput="__e"/><image hidden="{{!(!iconHidden)}}" class="img30" src="../../static/images/index/cancle.png" data-event-opts="{{[['tap',[['resetInput',['$event']]]]]}}" onTap="__e"></image></view><view data-event-opts="{{[['tap',[['showPicker',['$event']]]]]}}" class="s-input s-select" onTap="__e"><text class="text  wid27">{{text[0]}}</text><image class="img30 pdr20" src="../../static/images/index/icon_right.png"></image><text class="text  wid27">{{text[1]}}</text><image class="img30 pdr20" src="../../static/images/index/icon_right.png"></image><text class="text wid46">{{text[2]}}</text><image class="img32 flr" src="../../static/images/index/icon_down.png"></image></view></view><view class="btn-select"><button class="btn-primary" hover-class="btn-hover" data-event-opts="{{[['tap',[['showPicker',['$event']]]]]}}" onTap="__e">请选择</button></view><view data-event-opts="{{[['tap',[['hidePicker',['$event']]]]]}}" class="{{(('tui-mask-screen')+' '+(showPickerStatus?'tui-mask-show':''))}}" onTap="__e"></view><view class="{{(('tui-picker-box')+' '+(showPickerStatus?'tui-pickerbox-show':''))}}"><view class="picker-header list-item"><view class="btn-cancle" hover-class="opcity" hover-stay-time="{{150}}" data-event-opts="{{[['tap',[['hidePicker',['$event']]]]]}}" catchTap="__e">取消</view><view class="btn-sure" hover-class="opcity" hover-stay-time="{{150}}" data-event-opts="{{[['tap',[['picker',['$event']]]]]}}" catchTap="__e">确定</view></view><picker-view class="picker-view" indicator-style="height: 50px;" value="{{value}}" data-event-opts="{{[['change',[['columnPicker',['$event']]]]]}}" onChange="__e"><picker-view-column><block a:for="{{proviceArr}}" a:for-item="item" a:for-index="index" a:key="index"><view class="item">{{item}}</view></block></picker-view-column><picker-view-column><block a:for="{{cityArr}}" a:for-item="item" a:for-index="index" a:key="index"><view class="item">{{item}}</view></block></picker-view-column><picker-view-column><block a:for="{{districtArr}}" a:for-item="item" a:for-index="index" a:key="index"><view class="item">{{item}}</view></block></picker-view-column></picker-view></view></view>