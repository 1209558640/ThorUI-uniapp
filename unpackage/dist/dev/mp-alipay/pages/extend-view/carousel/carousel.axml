<view class="container"><view class="tui-banner-box"><swiper class="tui-banner-swiper" indicator-dots="{{true}}" autoplay="{{true}}" interval="{{5000}}" duration="{{150}}" circular="{{true}}" previous-margin="60rpx" next-margin="60rpx" data-event-opts="{{[['change',[['change',['$event']]]]]}}" onChange="__e"><block a:for="{{banner}}" a:for-item="item" a:for-index="index" a:key="index"><swiper-item class="tui-banner-item"><image class="{{(('tui-slide-image')+' '+(current!=index?'tui-banner-scale':''))}}" src="{{'../../../static/images/mall/banner/'+item}}" mode="scaleToFill" data-event-opts="{{[['tap',[['detail',['$event']]]]]}}" onTap="__e"></image></swiper-item></block></swiper></view><view class="tui-rolling-news list-item"><tui-icon vue-id="1" name="news" size="{{22}}" color="#555" onVueInit="__l"></tui-icon><swiper class="tui-swiper" vertical="{{true}}" autoplay="{{true}}" circular="{{true}}" interval="{{4000}}"><block a:for="{{headlines}}" a:for-item="item" a:for-index="index" a:key="index"><swiper-item class="tui-swiper-item"><view class="tui-news-item">{{item}}</view></swiper-item></block></swiper></view><view class="tui-classify-box"><block a:for="{{classify}}" a:for-item="item" a:for-index="index" a:key="index"><view class="tui-classify-item" data-key="{{item.name}}" data-event-opts="{{[['tap',[['more',['$event']]]]]}}" onTap="__e"><image class="tui-classify-img" src="{{'../../../static/images/classify/'+item.img+'.png'}}"></image><view class="tui-classify-name">{{item.name}}</view></view></block></view><view class="tui-spike-box"><view class="tui-spike-title list-item">限时秒杀</view><swiper class="tui-spike-swiper" indicator-dots="{{false}}" autoplay="{{true}}" interval="{{5000}}" duration="{{400}}" circular="{{true}}" display-multiple-items="{{2}}" vertical="{{true}}"><block a:for="{{productList}}" a:for-item="item" a:for-index="index" a:key="index"><swiper-item><view class="tui-pro-item list-item" hover-class="hover" hover-start-time="{{150}}" data-event-opts="{{[['tap',[['detail',['$event']]]]]}}" onTap="__e"><image class="tui-pro-img" src="{{'../../../static/images/mall/product/'+item.img+'.jpg'}}" mode="widthFix"></image><view class="tui-pro-content"><view class="tui-pro-tit">{{item.name}}</view><view class="tui-pro-btmbox"><view class="tui-pro-price"><text class="tui-sale-price">{{"￥"+item.sale}}</text><text class="tui-factory-price">{{"￥"+item.factory}}</text></view><view class="tui-countdown"><view class="tui-countdown-text">剩余</view><tui-countdown vue-id="{{'2-'+index}}" time="{{item.time}}" color="#e41f19" bcolor="#e41f19" colonColor="#e41f19" scale="{{true}}" onVueInit="__l"></tui-countdown><view class="tui-countdown-text">结束</view></view></view></view></view></swiper-item></block></swiper></view></view>