<view><block a:if="{{mask}}"><view data-event-opts="{{[['tap',[['handleClose',['$event']]]]]}}" class="{{(('tui-keyboard-mask')+' '+(show?'tui-mask-show':''))}}" onTap="__e"></view></block><view class="{{((('tui-keyboard')+' '+(action?'tui-keyboard-action':''))+' '+(show?'tui-keyboard-show':''))}}"><slot></slot><view class="tui-keyboard-grids"><block a:for="{{$root.l0}}" a:for-item="item" a:for-index="index" a:key="index"><view class="{{(('tui-keyboard-grid')+' '+(index==9||index==11?'tui-bg-gray':''))}}" hover-class="tui-keyboard-hover" hover-stay-time="{{150}}" data-index="{{index}}" data-event-opts="{{[['tap',[['handleClick',['$event']]]]]}}" onTap="__e"><block a:if="{{index<11}}"><view class="{{(('tui-keyboard-item')+' '+(index==9?'tui-fontsize-32':''))}}">{{item.m0}}</view></block><block a:else><view class="tui-keyboard-item"><view class="tui-icon tui-keyboard-delete"></view></view></block></view></block></view></view></view>