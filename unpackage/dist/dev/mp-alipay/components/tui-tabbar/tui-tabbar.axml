<view class="{{((('tui-tabbar')+' '+((isFixed)?'tui-tabbar-fixed':''))+' '+((unlined)?'tui-unlined':''))}}" style="{{'background:'+(backgroundColor)+';'}}"><block a:for="{{tabBar}}" a:for-item="item" a:for-index="index" a:key="index"><block><view data-event-opts="{{[['tap',[['tabbarSwitch',[index,'$0','$1','$2'],[[['tabBar','',index,'hump']],[['tabBar','',index,'pagePath']],[['tabBar','',index,'verify']]]]]]]}}" class="{{(('tui-tabbar-item')+' '+((item.hump)?'tui-item-hump':''))}}" style="{{'background-color:'+(item.hump?backgroundColor:'none')+';'}}" onTap="__e"><view class="{{(('tui-icon-box')+' '+((item.hump)?'tui-tabbar-hump':''))}}"><image class="{{(item.hump?'':'tui-tabbar-icon')}}" src="{{current==index?item.selectedIconPath:item.iconPath}}"></image><block a:if="{{item.num}}"><view class="{{(item.isDot?'tui-badge-dot':'tui-badge')}}" style="{{'color:'+(badgeColor)+';'+('background-color:'+(badgeBgColor)+';')}}">{{item.isDot?"":item.num}}</view></block></view><view class="{{(('tui-text-scale')+' '+((item.hump)?'tui-text-hump':''))}}" style="{{'color:'+(current==index?selectedColor:color)+';'}}">{{item.text}}</view></view></block></block><block a:if="{{hump&&!unlined}}"><view class="{{((hump)?'tui-hump-box':'')}}"></view></block></view>