<view class="tui-keyboard-input tui-pwd-box" style="{{'background:'+(bgcolor)+';'}}"><view class="tui-inner-box"><block a:for="{{inputvalue}}" a:for-item="item" a:for-index="index" a:key="index"><view class="{{(('tui-input')+' '+(inputvalue.length===4?'tui-margin-right':''))}}" style="{{'font-size:'+($root.m0)+';'+('color:'+(color)+';')+('width:'+($root.m1)+';')}}">{{item}}</view></block></view></view>