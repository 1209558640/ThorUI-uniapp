<view class="tui-datetime-picker"><view class="{{(('tui-mask')+' '+((isShow)?'tui-mask-show':''))}}" catchtouchmove="stop" data-event-opts="{{[['touchMove',[['stop',['$event']]]],['tap',[['hide',['$event']]]]]}}" catchTouchMove="__e" onTap="__e"></view><view class="{{(('tui-header')+' '+((isShow)?'tui-show':''))}}"><view class="tui-picker-header" catchtouchmove="stop" data-event-opts="{{[['touchMove',[['stop',['$event']]]]]}}" catchTouchMove="__e"><view class="tui-btn-picker" style="{{'color:'+(cancelColor)+';'}}" hover-class="tui-opacity" hover-stay-time="{{150}}" data-event-opts="{{[['tap',[['hide',['$event']]]]]}}" onTap="__e">取消</view><view class="tui-btn-picker" style="{{'color:'+(color)+';'}}" hover-class="tui-opacity" hover-stay-time="{{150}}" data-event-opts="{{[['tap',[['btnFix',['$event']]]]]}}" onTap="__e">确定</view></view><view class="tui-picker-body"><picker-view class="tui-picker-view" value="{{value}}" data-event-opts="{{[['change',[['change',['$event']]]]]}}" onChange="__e"><block a:if="{{!reset&&type!=4}}"><picker-view-column><block a:for="{{years}}" a:for-item="item" a:for-index="index" a:key="index"><view class="tui-column-item">{{''+item}}<text class="tui-text">年</text></view></block></picker-view-column></block><block a:if="{{!reset&&type!=4}}"><picker-view-column><block a:for="{{$root.l0}}" a:for-item="item" a:for-index="index" a:key="index"><view class="tui-column-item">{{''+item.m0}}<text class="tui-text">月</text></view></block></picker-view-column></block><block a:if="{{!reset&&(type==1||type==2)}}"><picker-view-column><block a:for="{{$root.l1}}" a:for-item="item" a:for-index="index" a:key="index"><view class="tui-column-item">{{''+item.m1}}<text class="tui-text">日</text></view></block></picker-view-column></block><block a:if="{{!reset&&(type==1||type==4)}}"><picker-view-column><block a:for="{{$root.l2}}" a:for-item="item" a:for-index="index" a:key="index"><view class="tui-column-item">{{''+item.m2}}<text class="tui-text">时</text></view></block></picker-view-column></block><block a:if="{{!reset&&(type==1||type==4)}}"><picker-view-column><block a:for="{{$root.l3}}" a:for-item="item" a:for-index="index" a:key="index"><view class="tui-column-item">{{''+item.m3}}<text class="tui-text">分</text></view></block></picker-view-column></block></picker-view></view></view></view>