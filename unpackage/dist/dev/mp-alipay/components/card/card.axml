<view data-event-opts="{{[['tap',[['handleClick',['$event']]]],['longTap',[['longTap',['$event']]]]]}}" class="{{((('tui-card-class tui-card')+' '+(full?'tui-card-full':''))+' '+(border?'tui-card-border':''))}}" onTap="__e" onLongTap="__e"><view class="{{(('tui-card-header')+' '+((header.line)?'tui-header-line':''))}}" style="{{'background:'+(header.bgcolor||'#fff')+';'}}"><view class="tui-header-left"><block a:if="{{image.url}}"><image class="{{(('tui-header-thumb')+' '+((image.circle)?'tui-thumb-circle':''))}}" style="{{'height:'+($root.m0)+';'+('width:'+($root.m1)+';')}}" src="{{image.url}}" mode="widthFix"></image></block><block a:if="{{title.text}}"><text class="tui-header-title" style="{{'font-size:'+($root.m2)+';'+('color:'+(title.color||'#7A7A7A')+';')}}">{{title.text}}</text></block></view><block a:if="{{tag.text}}"><view class="tui-header-right" style="{{'font-size:'+($root.m3)+';'+('color:'+(tag.color||'#b2b2b2')+';')}}">{{''+tag.text+''}}</view></block></view><view class="tui-card-body"><slot name="body"></slot></view><view class="tui-card-footer"><slot name="footer"></slot></view></view>