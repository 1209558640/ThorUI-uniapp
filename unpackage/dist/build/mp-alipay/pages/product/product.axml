<view class="container"><view class="product-list"><block a:for="{{productList}}" a:for-item="item" a:for-index="index" a:key="index"><view data-event-opts="{{[['tap',[['detail',['$event']]]]]}}" class="pro-item" onTap="__e"><image class="pro-img" src="{{'../../static/images/product/'+item.img+'.jpg'}}" mode="widthFix"></image><view class="pro-content"><view class="pro-tit">{{item.name}}</view><view><view class="pro-price"><text class="sale-price">{{"￥"+item.sale}}</text><text class="factory-price">{{"￥"+item.factory}}</text></view><view class="pro-pay">{{item.payNum+"人付款"}}</view></view></view></view></block></view><tui-loadmore vue-id="1" visible="{{loadding}}" index="{{3}}" type="primary" onVueInit="__l"></tui-loadmore><tui-nomore vue-id="2" visible="{{!pullUpOn}}" onVueInit="__l"></tui-nomore></view>