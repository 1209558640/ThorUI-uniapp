<view class="tui-container"><view class="tui-extend-box"><block a:for="{{list}}" a:for-item="item" a:for-index="index" a:key="index"><block a:if="{{(index+1)%2!=0}}"><block><view class="{{(('tui-extend-item')+' '+('tui-bg-'+item.bg))}}" id="{{item.page}}" data-event-opts="{{[['tap',[['detail',['$event']]]]]}}" catchTap="__e"><view class="tui-title tui-light">{{''+item.name+''}}</view><view class="tui-sub-title">{{item.desc}}</view><view class="tui-footer"><tui-tag vue-id="{{'1-'+index}}" size="small" type="white" shape="circle" plain="{{true}}" onVueInit="__l">查看详情</tui-tag><view><button data-event-opts="{{[['tap',[['like',[index]]]]]}}" class="tui-btn" catchTap="__e"><tui-icon class="tui-l-icon" vue-id="{{'2-'+index}}" name="{{item.like?'like-fill':'like'}}" color="{{item.like?'#f54f46':'#fff'}}" size="{{20}}" onVueInit="__l"></tui-icon></button><button class="tui-btn" open-type="share" data-id="{{index}}" data-event-opts="{{[['tap',[['onshare',['$event']]]]]}}" catchTap="__e"><tui-icon class="tui-r-icon" vue-id="{{'3-'+index}}" name="partake" color="#fff" size="{{20}}" onVueInit="__l"></tui-icon></button></view></view></view></block></block></block></view><view class="tui-extend-box"><block a:for="{{list}}" a:for-item="item" a:for-index="index" a:key="index"><block a:if="{{(index+1)%2==0}}"><block><view class="{{(('tui-extend-item')+' '+('tui-bg-'+item.bg))}}" id="{{item.page}}" data-event-opts="{{[['tap',[['detail',['$event']]]]]}}" catchTap="__e"><view class="tui-title tui-light">{{''+item.name+''}}</view><view class="tui-sub-title">{{item.desc}}</view><view class="tui-footer"><tui-tag vue-id="{{'4-'+index}}" size="small" type="white" shape="circle" plain="{{true}}" onVueInit="__l">查看详情</tui-tag><view><button data-event-opts="{{[['tap',[['like',[index]]]]]}}" class="tui-btn" catchTap="__e"><tui-icon class="tui-l-icon" vue-id="{{'5-'+index}}" name="{{item.like?'like-fill':'like'}}" color="{{item.like?'#f54f46':'#fff'}}" size="{{20}}" onVueInit="__l"></tui-icon></button><button class="tui-btn" open-type="share" data-id="{{index}}" data-event-opts="{{[['tap',[['onshare',['$event']]]]]}}" catchTap="__e"><tui-icon class="tui-r-icon" vue-id="{{'6-'+index}}" name="partake" color="#fff" size="{{20}}" onVueInit="__l"></tui-icon></button></view></view></view></block></block></block></view></view>