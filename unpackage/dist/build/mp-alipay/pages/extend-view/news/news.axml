<view class="tui-container"><view class="tui-searchbox"><view class="tui-rolling-search"><icon type="search" size="{{13}}" color="#999"></icon><swiper class="tui-swiper" vertical autoplay circular interval="8000"><block a:for="{{hotSearch}}" a:for-item="item" a:for-index="index" a:key="index"><swiper-item data-event-opts="{{[['tap',[['search',['$event']]]]]}}" class="tui-swiper-item" onTap="__e"><view class="tui-hot-item">{{"大家正在搜："+item}}</view></swiper-item></block></swiper></view></view><swiper class="tui-banner-swiper" indicator-dots autoplay circular interval="{{5000}}" duration="{{150}}" indicator-color="rgba(255, 255, 255, 0.9)" indicator-active-color="#5677fc"><block a:for="{{banner}}" a:for-item="item" a:for-index="index" a:key="index"><swiper-item data-event-opts="{{[['tap',[['bannerDetail',['$event']]]]]}}" catchTap="__e"><view class="tui-banner-title">{{item.title}}</view><image class="tui-slide-image" src="{{'../../../static/images/news/'+item.img}}" mode="widthFix"></image></swiper-item></block></swiper><view class="tui-news-view"><block a:for="{{newsList}}" a:for-item="item" a:for-index="index" a:key="index"><block><tui-list-cell vue-id="{{'1-'+index}}" index="{{index}}" last="{{count==index}}" data-event-opts="{{[['^click',[['detail']]]]}}" onClick="__e" onVueInit="__l"><view class="{{(('tui-news-flex')+' '+(item.isVideo||item.imgNum>1?'tui-flex-column':'tui-flex-start'))}}"><block a:if="{{item.imgNum>0}}"><view class="{{((('tui-news-picbox')+' '+(item.isVideo||item.imgNum>1?'tui-w-full':'tui-w220 tui-h165'))+' '+(item.imgNum>1?'tui-flex-between':''))}}"><block a:for="{{item.img}}" a:for-item="items" a:for-index="index2" a:key="index2"><block><image class="{{((('tui-block')+' '+(item.imgNum>1?'tui-one-third':''))+' '+(item.isVideo?'tui-w-full':''))}}" src="{{'../../../static/images/news/'+items}}" mode="widthFix"></image></block></block><block a:if="{{item.isVideo||item.imgNum>3}}"><view class="tui-btm-badge">{{item.isVideo?item.time:item.imgNum+'图'}}</view></block><block a:if="{{item.isVideo}}"><view class="tui-video"><tui-icon vue-id="{{('2-'+index)+','+('1-'+index)}}" name="play" color="#fff" size="{{24}}" onVueInit="__l"></tui-icon></view></block></view></block><view class="{{(('tui-news-tbox tui-flex-column tui-flex-between')+' '+(item.imgNum===1&&!item.isVideo?'tui-h165 tui-pl-20':''))}}"><view class="{{(('tui-news-title')+' '+(!item.isVideo&&item.imgNum===1||item.imgNum===0?'':'tui-pt20'))}}">{{item.title}}</view><view class="{{(('tui-sub-box')+' '+(!item.isVideo&&item.imgNum===1?'':'tui-pt20'))}}"><view class="tui-sub-source">{{item.source}}</view><view class="tui-sub-cmt"><view>{{item.cmtsNum+"评论"}}</view><view class="tui-scale"><block a:if="{{item.isTop}}"><tui-tag vue-id="{{('3-'+index)+','+('1-'+index)}}" size="small" plain="{{true}}" shape="circleRight" onVueInit="__l">置顶</tui-tag></block></view></view></view></view></view></tui-list-cell></block></block></view><tui-tips vue-id="4" ref="__r" data-ref="toast" onVueInit="__l"></tui-tips><tui-loadmore vue-id="5" visible="{{loadding}}" index="{{3}}" type="primary" onVueInit="__l"></tui-loadmore><tui-nomore vue-id="6" visible="{{!pullUpOn}}" bgcolor="#f2f2f2" onVueInit="__l"></tui-nomore></view>