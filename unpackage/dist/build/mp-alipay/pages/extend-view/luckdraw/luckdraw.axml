<view class="container"><block a:for="{{circleList}}" a:for-item="item" a:for-index="index" a:key="index"><view class="{{(('tui-dot')+' '+('tui-dot-'+(index+1)))}}"></view></block><view class="tui-container-in"><block a:for="{{awardList}}" a:for-item="item" a:for-index="index" a:key="index"><view class="{{((('tui-content-out')+' '+('tui-award-'+(index+1)))+' '+(index==indexSelect?'tui-awardSelect':''))}}"><image class="tui-award-image" src="{{'../../../static/images/luck/'+item.img}}"></image></view></block><view data-event-opts="{{[['tap',[['startDrawing',['$event']]]]]}}" class="{{(('tui-btn-start')+' '+(isRunning?'tui-ative':''))}}" onTap="__e">立即抽奖</view></view></view>