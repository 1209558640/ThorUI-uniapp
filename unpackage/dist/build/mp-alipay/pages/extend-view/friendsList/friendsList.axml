<view class="tui-page"><scroll-view class="tui-scrollList" style="{{'height:'+(winHeight+'px')+';'}}" scroll-y scroll-into-view="{{scrollViewId}}" data-event-opts="{{[['scroll',[['onScroll',['$event']]]]]}}" onScroll="__e"><view class="tui-searchbox"><view data-event-opts="{{[['tap',[['search',['$event']]]]]}}" class="tui-search-input" onTap="__e"><icon type="search" size="{{15}}" color="#999"></icon><text class="tui-search-text">搜索</text></view></view><tui-cell vue-id="1" last="{{true}}" data-event-opts="{{[['^click',[['detail']]]]}}" onClick="__e" onVueInit="__l"><image class="tui-img" src="../../../static/images/news/2.jpg"></image><view class="tui-name">新的朋友</view></tui-cell><view class="tui-list city-list"><block a:for="{{$root.l1}}" a:for-item="list" a:for-index="index" a:key="index"><block><block a:if="{{list.$orig.data[0]}}"><tui-sticky vue-id="{{'2-'+index}}" scrollTop="{{scrollTop}}" stickyHeight="66rpx" onVueInit="__l"><view slot="header"><view class="tui-list-cell-divider" id="{{list.$orig.letter}}">{{''+list.$orig.letter+''}}</view></view><view slot="content"><block a:for="{{list.l0}}" a:for-item="item" a:for-index="index2" a:key="index2"><block><tui-cell vue-id="{{('3-'+index+'-'+index2)+','+('2-'+index)}}" last="{{item.m0}}" data-event-opts="{{[['^click',[['detail']]]]}}" onClick="__e" onVueInit="__l"><image class="tui-img" src="{{'../../../static/images/news/'+(index2%2===0&&index2!==0?'avatar_1.jpg':'avatar_2.jpg')}}"></image><view class="tui-name">{{item.$orig.name}}</view></tui-cell></block></block></view></tui-sticky></block></block></block></view><view class="tui-footer">120位联系人</view><view class="tui-safearea-bottom"></view></scroll-view><view data-event-opts="{{[['touchStart',[['touchStart',['$event']]]],['touchMove',[['touchMove',['$event']]]],['touchEnd',[['touchEnd',['$event']]]],['touchCancel',[['touchCancel',['$event']]]]]}}" class="tui-indexed-list-bar" style="{{'height:'+(indexBarHeight+'px')+';'}}" catchTouchStart="__e" catchTouchMove="__e" catchTouchEnd="__e" catchTouchCancel="__e"><block a:for="{{lists}}" a:for-item="items" a:for-index="index3" a:key="index3"><view class="tui-indexed-list-text" style="{{'height:'+(indexBarItemHeight+'px')+';'}}">{{''+(items.letter=="well"?"#":items.letter)+''}}</view></block></view><block a:if="{{touchmove&&lists[touchmoveIndex].letter}}"><view class="tui-indexed-list-alert"><text>{{lists[touchmoveIndex].letter=="well"?"#":lists[touchmoveIndex].letter}}</text></view></block></view>