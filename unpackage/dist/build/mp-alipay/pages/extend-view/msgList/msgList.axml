<view class="container"><view class="tui-tabbar"><block a:for="{{tabbar}}" a:for-item="item" a:for-index="index" a:key="index"><block><view class="{{(('tui-tabbar-item')+' '+(current==index?'tui-item-active':''))}}" data-index="{{index}}" data-event-opts="{{[['tap',[['tabbarSwitch',['$event']]]]]}}" onTap="__e"><tui-icon class="{{(index==0?'tui-ptop-4':'')}}" vue-id="{{'1-'+index}}" name="{{current==index?item.icon+'-fill':item.icon}}" color="{{current==index?'#00C0FB':'#666'}}" size="{{item.size}}" onVueInit="__l"></tui-icon><view class="tui-scale">{{item.text}}</view></view></block></block></view><view class="tui-searchbox"><view data-event-opts="{{[['tap',[['search',['$event']]]]]}}" class="tui-search-input" onTap="__e"><icon type="search" size="15" color="#999"></icon><text class="tui-search-text">搜索</text></view></view><block a:for="{{msgList}}" a:for-item="item" a:for-index="index" a:key="index"><block><tui-list-cell vue-id="{{'2-'+index}}" last="{{true}}" data-event-opts="{{[['^click',[['detail']]]]}}" onClick="__e" onVueInit="__l"><view class="tui-msg-box"><image class="tui-msg-pic" src="{{'../../../static/images/news/'+item.pic+'.jpg'}}" mode="widthFix"></image><view class="tui-msg-item"><view class="tui-msg-name">{{item.nickname}}</view><view class="tui-msg-content">{{item.msg}}</view></view></view><view class="{{(('tui-msg-right')+' '+(item.level==3?'tui-right-dot':''))}}"><view class="tui-msg-time">{{item.time}}</view><block a:if="{{item.msgNum>0}}"><tui-badge vue-id="{{('3-'+index)+','+('2-'+index)}}" type="{{item.level==2?'gray':'danger'}}" dot="{{item.level==3?true:false}}" onVueInit="__l">{{item.level!=3?item.msgNum:""}}</tui-badge></block></view></tui-list-cell></block></block><view class="tui-safearea-bottom"></view></view>