<view class="container"><view class="btn-box"><button class="btn-primary" hover-class="btn-hover" data-event-opts="{{[['tap',[['showModal',['$event']]]]]}}" onTap="__e">从底部弹出</button></view><view class="btn-box "><button class="btn-primary" hover-class="btn-hover" data-event-opts="{{[['tap',[['rDrawer',['$event']]]]]}}" onTap="__e">从右边弹出</button></view><view class="btn-box "><button class="btn-primary" hover-class="btn-hover" data-event-opts="{{[['tap',[['lDrawer',['$event']]]]]}}" onTap="__e">从左边弹出</button></view><view data-event-opts="{{[['tap',[['hideModal',['$event']]]]]}}" hidden="{{!(showModalStatus)}}" class="mask-screen" onTap="__e"></view><view hidden="{{!(showModalStatus)}}" class="region-box" animation="{{animationData}}"><block a:for="{{regionArr}}" a:for-item="item" a:for-index="index"><view class="{{((('region-txt')+' '+(index==regionArr.length-3||index==regionArr.length-2||index==regionArr.length-1?'grow':''))+' '+(tabIndex==index?'active':''))}}" data-index="{{index}}" data-event-opts="{{[['tap',[['getRegion',['$event']]]]]}}" catchTap="__e">{{item}}</view></block></view><tui-drawer vue-id="1" mode="left" visible="{{leftDrawer}}" data-event-opts="{{[['^close',[['closeDrawer']]]]}}" onClose="__e" onVueInit="__l"><view class="d-container"><button class="btn-primary" hover-class="btn-hover" data-event-opts="{{[['tap',[['closeDrawer',['$event']]]]]}}" onTap="__e">关闭抽屉</button></view></tui-drawer><tui-drawer vue-id="2" mode="right" visible="{{rightDrawer}}" data-event-opts="{{[['^close',[['closeDrawer']]]]}}" onClose="__e" onVueInit="__l"><view class="d-container"><button class="btn-primary" hover-class="btn-hover" data-event-opts="{{[['tap',[['closeDrawer',['$event']]]]]}}" onTap="__e">关闭抽屉</button></view></tui-drawer></view>