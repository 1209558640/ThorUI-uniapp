<block a:if="{{position=='top'}}"><block><view class="{{((('tui-tips-class tui-toptips')+' '+('tui-'+type))+' '+(show?'tui-top-show':''))}}">{{msg}}</view></block></block><block a:else><block><view class="{{((('tui-tips-class tui-toast')+' '+(position=='center'?'tui-centertips':'tui-bottomtips'))+' '+(show?'tui-toast-show':''))}}"><view class="{{(('tui-tips-content')+' '+('tui-'+type))}}">{{''+msg+''}}</view></view></block></block>